<Window x:Class="Login.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Login"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="lblUsername" Content="Username" HorizontalAlignment="Left" Margin="148,126,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbxUsername" HorizontalAlignment="Left" Height="23" Margin="256,126,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblPassword" Content="Password" HorizontalAlignment="Left" Margin="148,181,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbxPassword" HorizontalAlignment="Left" Height="23" Margin="256,181,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnLogin" Content="Login" HorizontalAlignment="Left" Margin="328,250,0,0" VerticalAlignment="Top" Width="126" Height="34" Click="BtnLogin_Click"/>
        <Button x:Name="btnLogin_Copy" Content="Register" HorizontalAlignment="Left" Margin="134,250,0,0" VerticalAlignment="Top" Width="126" Height="34" Click="BtnLogin_Copy_Click"/>

    </Grid>
</Window>
